  <question type="multichoice">
    <name>
      <text>Q12.3</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Which of the following statements about SQL Database and Windows Azure Table Storage are true</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
